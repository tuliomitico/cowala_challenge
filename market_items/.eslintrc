{
  "env": {
      "es2021": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "camelcase": 0,
      "@typescript-eslint/camelcase": 0,
      "no-useless-constructor": 0,
      "@typescript-eslint/no-useless-constructor": 0
  }
}
